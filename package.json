{
	"name": "vscode-live2d",
	"displayName": "vscode-live2d",
	"description": "vscode看板娘",
	"version": "0.0.1",
	"publisher": "CharlesZ",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"vscode看板娘",
		"看板娘",
		"live2d",
		"canvas"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.vslive2d",
				"title": "Hello vslive2d"
			}
		],
		"configuration": {
			"title": "vscode看板娘设置",
			"type": "Object",
			"properties": {
				"vslive2d.enabled": {
					"type": "boolean",
					"default": true,
					"description": "插件是否启用"
				},
				"vslive2d.model": {
					"type": "string",
					"enum": [
						"shizuku",
						"shizuku-pajama",
						"22",
						"33"
					],
					"default": "shizuku",
					"description": "选哪个娘"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}